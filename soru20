package ornek;
public class soru20 {
	public static void main(String[] args) {
		int[]dizi= {64,32,64,43,23,12,54};  
		System.out.println("Çift Sayılar -> ");
		for(int i=0; i<7; i++)
		{
			if(dizi[i]%2==0)
			{
				System.out.println(dizi[i]+" Sayısı Çifttir");
			}
			else
				System.out.println(dizi[i]+" Sayısı Tektir");
		}
	}
}
