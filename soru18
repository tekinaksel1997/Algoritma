package ornek;
import java.util.*;
public class soru18 {

	public static void main(String[] args) {
		System.out.println("Celcius derecesi girin : ");
		Scanner scan=new Scanner(System.in);
		double c=scan.nextDouble();
		double f=32+c*9/5;
		System.out.print(c+" Celcius : "+f+" Fahrenheit");
	}

}
